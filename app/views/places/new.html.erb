<h2>Nouveau lieu</h2>

<%= form_for :place, url: places_path do |f| %>
    <p>
      <%= f.label :label %>
      <%= f.text_field :label %>
    </p>
    <p>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= collection_select(:place, :stand_id, @stands, :id, :name, include_blank: true) %>
    </p>
    <%= f.submit %>
<% end %>