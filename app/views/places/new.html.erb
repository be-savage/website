<div class="page-header">
  <h1>Nouveau lieu</h1>
</div>

<%= simple_form_for @place, html: { class: 'form-horizontal' }, wrapper: :horizontal_form, url: places_path do |f| %>
    <%= f.input :label %>
    <%= f.input :description %>
    <%= f.association :stand %>

    <%= f.button :submit %>
<% end %>